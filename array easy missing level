class Solution{
    public static void main(String[] args){
        int[] arr1={1,5,3,4};
        int n=arr1.length+1;
        int i=0;
        int sum=0;
        int nsum=0;
        for(i=0;i<arr1.length;i++){
             sum += arr1[i];
        }
        nsum=n*(n+1)/2;
        int result=nsum-sum;
        System.out.println(result+"");
        
    }
}
