class Solution {
    public void pattern3(int n) {
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print(j);
            }
            System.out.println("");
        }

    }
    public static void main(String []args){
        Scanner sc =new Scanner(System.in);
        int n=sc.nextInt();
        System.out.println("enter n value");
        Solution s=new Solution();
        s.pattern3(n);
    }
}
